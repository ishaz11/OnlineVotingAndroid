@model IEnumerable<OnlineVotingAndroid.Models.Candidate>

@{
    ViewBag.Title = "ActiveElection";
}




<div class="card-header">
    <h4 class="card-title">President</h4>
</div>
<div class="card-body">
    <div class="table-responsive">
        <table class="table mb-0">
            <thead>
                <tr>
                    <th>
                        Student Name
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Students.StudentID)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Position.PositionName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Students.StudentID)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.CandidateID }) |
                            @Html.ActionLink("Details", "Details", new { id = item.CandidateID }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.CandidateID })
                        </td>
                    </tr>
                }
            </tbody>


        </table>
    </div>
</div>
